{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "indent": "warn",
        "react/prop-types": "off",
        "import/no-anonymous-default-export": "off",
        "no-mixed-spaces-and-tabs": "off",
        "react/no-unescaped-entities": "off",
        "react/react-in-jsx-scope": "off",
        "no-unused-vars": "off",
        "no-undef": "off",
        "react/no-unknown-property": "off"
    }
}
